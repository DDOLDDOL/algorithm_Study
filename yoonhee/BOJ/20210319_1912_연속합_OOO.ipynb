    "num = int(input())\n",
    "num_list = list(map(int, input().split()))\n",
    "dp =[0]*num # 배열 범위 설정 \n",
    "dp[0] = num_list[0] #값비교를 위한 list\n",
    "\n",
    "for i in range(1,num):\n",
    "    dp[i] = max(num_list[i], dp[i-1] + num_list[i])\n",
    "print(max(dp))"
   ]
